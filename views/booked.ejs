<main class="main">

  <!-- Page Title -->
  <div class="page-title">
    <div class="heading">
      <div class="container">
        <div class="row d-flex justify-content-center text-center">
          <div class="col-lg-8">
            <br>
            <h1 class="heading-title">Registered & Contacted Details</h1>
          </div>
        </div>
      </div>
    </div>
  </div><!-- End Page Title -->

  <!-- registration data storage -->
  <section id="speaker-details-2" class="speaker-details-2 section">

    <div class="container" data-aos="fade-up" data-aos-delay="100">

      <div class="row justify-content-center">
        <div class="col-lg-8">



          <div class="speaker-sessions mb-5" data-aos="fade-up" data-aos-delay="500">
            <h3 class="sessions-title mb-4">Registered Details</h3>
            <div class="sessions-list">

              <% registrationData.forEach((r) => { %>
              <div class="session-card" data-aos="fade-up" data-aos-delay="600">
                <div class="session-content">
                  <!-- <div class="session-type">Keynote</div> -->
                  <!-- <h4 class="session-title">The Future of Digital Transformation</h4> -->
                  <!-- <p class="session-description">Lorem ipsum dolor sit amet, consectetur adipiscing elit. Sed do eiusmod
                    tempor incididunt ut labore et dolore magna aliqua.</p> -->
                  <div class="session-meta">
                    <div class="session-info">

                      <div class="session-detail">
                        <i class="bi bi-person-bounding-box"></i>
                        <span><b>Performance:-</b>  <%= r.performance %></span>
                      </div>

                      <div class="session-detail">
                        <i class="bi bi-person"></i>
                        <span><b>Name:-</b>  <%= r.name %></span>
                      </div>
                      <div class="session-detail">
                        <i class="bi bi-people"></i>
                        <span><b>Team Name:-</b>  <%= r.team_name %></span>
                      </div>
                      <div class="session-detail">
                        <i class="bi bi-telephone"></i>
                        <span><b>Phone Number:</b>  <%= r.phone %></span>
                      </div>
                      <div class="session-detail">
                        <i class="bi bi-telephone-outbound"></i>
                        <span><b>Alternate Phone Number:-</b>  <%= r.alt_phone %></span>
                      </div>
                      <div class="session-detail">
                        <i class="bi bi-building"></i>
                        <span><b>College Name:-</b>  <%= r.college_name %></span>
                      </div>
                      <div class="session-detail">
                        <i class="bi bi-journal-bookmark"></i>
                        <span><b>Department:-</b>  <%= r.department %></span>
                      </div>
                      <div class="session-detail">
                        <i class="bi bi-people-fill"></i>
                        <span><b>Number of participants:-</b>  <%= r.no_of_participants %></span>
                      </div>
                      <div class="session-detail">
                        <i class="bi bi-receipt"></i>
                        <span><b>Transaction ID:-</b>  <%= r.transaction_id %></span>
                      </div>
                      <div class="session-detail">
                        <i class="bi bi-file-earmark-text"></i>
                        <span><b>Rules:-</b>  <%= r.rules %></span>
                      </div>
                      <div class="session-detail">
                        <i class="bi bi-calendar-check"></i>
                        <span><b>Registered At:-</b>  <%= r.createdAt.toLocaleString() %></span>
                      </div>
                    </div>
                  </div>
                </div>
              </div><!-- End Session Card -->
              <% }) %>

            </div>
          </div>

        </div>
      </div>

    </div>

  </section><!-- /registration data storage -->




  <!-- contact message data storage -->
  <section id="speaker-details-2" class="speaker-details-2 section">

    <div class="container" data-aos="fade-up" data-aos-delay="100">

      <div class="row justify-content-center">
        <div class="col-lg-8">



          <div class="speaker-sessions mb-5" data-aos="fade-up" data-aos-delay="500">
            <h3 class="sessions-title mb-4">Queries Contacted Details</h3>
            <div class="sessions-list">

              <% contactData.forEach((c) => { %>
              <div class="session-card" data-aos="fade-up" data-aos-delay="600">
                <div class="session-content">
                  <!-- <div class="session-type">Keynote</div>
                  <h4 class="session-title">The Future of Digital Transformation</h4>
                  <p class="session-description">Lorem ipsum dolor sit amet, consectetur adipiscing elit. Sed do eiusmod
                    tempor incididunt ut labore et dolore magna aliqua.</p> -->
                  <div class="session-meta">
                    <div class="session-info">
                      <div class="session-detail">
                        <i class="bi bi-person"></i>
                        <span><b>Name:-</b>  <%= c.name %></span>
                      </div>
                      <div class="session-detail">
                        <i class="bi bi-building"></i>
                        <span><b>College Name:-</b>  <%= c.college %></span>
                      </div>
                      <div class="session-detail">
                        <i class="bi bi-journal-bookmark"></i>
                        <span><b>Department:-</b>  <%= c.department %></span>
                      </div>
                      <div class="session-detail">
                        <i class="bi bi-telephone"></i>
                        <span><b>Phone Number:-</b>  <%= c.phone %></span>
                      </div>
                      <div class="session-detail">
                        <i class="bi bi-chat-square-text"></i>
                        <span><b>Subject:-</b>  <%= c.subject %></span>
                      </div>
                      <div class="session-detail">
                        <i class="bi bi-chat-left-dots"></i>
                        <span><b>Message:-</b>  <%= c.message %></span>
                      </div>
                      <div class="session-detail">
                        <i class="bi bi-calendar-check"></i>
                        <span><b>Sent At:-</b>  <%= c.createdAt.toLocaleString() %></span>
                      </div>
                    </div>
                  </div>
                </div>
              </div><!-- End Session Card -->
              <% }) %>

            </div>
          </div>

        </div>
      </div>

    </div>

  </section><!-- /Speaker Details 2 Section -->

</main>